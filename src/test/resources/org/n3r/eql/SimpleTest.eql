-- AutoSeq
-- [getStringWithTwoParams]
SELECT 'x'
FROM DUAL
WHERE 'x' = ##
  AND 'y' = ##

-- Custome Seq
-- [getStringWithTwoParamsAndSequence]
SELECT 'x'
FROM DUAL
WHERE 'x' = #2#
  AND 'y' = #1#


-- [queryXY]
select 'x' as X, 'y' as Y
  from DUAL

-- [queryXY2]
select 'x0' as X, 'y0' as Y
from DUAL
union all
select 'x1' as X, 'y1' as Y
from DUAL

-- [createTestTable]
CREATE TABLE ESQL_TEST (
  A INT NOT NULL,
  B CHAR(10),
  C VARCHAR(10),
  D TIMESTAMP,
  E int
);

INSERT INTO ESQL_TEST VALUES(1, 'A', '\#AC', ##, 101);
INSERT INTO ESQL_TEST VALUES(2, 'B', '\#BC', ##, 102);
INSERT INTO ESQL_TEST VALUES(3, 'C', 'CC', ##, 103);
INSERT INTO ESQL_TEST VALUES(4, 'D', 'DC', ##, 104);

-- [dropTestTable onerr=resume]
DROP TABLE ESQL_TEST;

-- [getBeanList]
SELECT A,B,C,D,E
FROM ESQL_TEST

-- [getStringList]
SELECT C
FROM ESQL_TEST

-- [updateBean]
UPDATE ESQL_TEST
SET B = #2#
WHERE A = #1#

-- [selectBean]
SELECT A,B,C,D,E
FROM ESQL_TEST
WHERE A = ##

-- [selectByBean]
SELECT A,B,C,D,E
FROM ESQL_TEST
WHERE A = #a#
AND C = #c#

